{
  "ssg": "astro",
  "mode": "hosted",
  "build": {
    "install_command": "npm install",
    "build_command": "npm run build",
    "output_path": "dist",
    "preserved_paths": "",
    "preserveOutput": false,
    "includeGit": true,
    "environment_variables": [
      {
        "key": "SYNC_PATHS",
        "value": "/rosey/"
      },
      {
        "key": "TRANSLATE",
        "value": false
      },
      {
        "key": "LOCALES",
        "value": "fr-FR,de-DE,es-ES"
      },
      {
        "key": "SMARTLING_TRANSLATION_ENABLED",
        "value": false
      }
    ]
  }
}
